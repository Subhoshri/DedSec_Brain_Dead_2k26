  gpt2_config.is_decoder = True\n        \n        self.text_decoder = GPT2LMHeadModel.from_pretrained(\n            'gpt2',\n            config=gpt2_config,\n            cache_dir='/kaggle/working/transformers_cache'\n        )\n        self.text_decoder.resize_token_embeddings(50257)\n        \n        self.visual_to_gpt2 = nn.Linear(512, 768)\n    \n    def forward(self, pixel_values, input_ids=None, attention_mask=None):\n        batch_size = pixel_values.size(0)\n        \n        # MODULE 1: Hierarchical Visual Features\n        vit_outputs = self.visual_encoder(pixel_values, output_hidden_states=True)\n        \n        # Pixel level\n        pixel_features = vit_outputs.last_hidden_state[:, 1:]\n        pixel_features = self.pixel_proj(pixel_features)\n        \n        # Region level\n        region_features = pixel_features.reshape(batch_size, 14, 14, 512)\n        region_features = nn.functional.avg_pool2d(\n            region_features.permute(0, 3, 1, 2), \n            kernel_size=4, \n            stride=4\n        ).permute(0, 2, 3, 1).reshape(batch_size, -1, 512)\n        \n        # Organ level\n        organ_features = vit_outputs.last_hidden_state[:, 0:1]\n        organ_features = self.organ_proj(organ_features)\n        \n        # MODULE 2: Disease Classification\n        global_feature = vit_outputs.last_hidden_state[:, 0]\n        residual_out = self.residual_path(global_feature)\n        expansion_out = self.expansion_path(global_feature)\n        disease_features = residual_out + expansion_out\n        disease_logits = self.disease_classifier(disease_features)\n        \n        # MODULE 3: Triangular Cognitive Attention\n        combined_visual = torch.cat([pixel_features, region_features, organ_features], dim=1)\n        disease_embed = disease_features.unsqueeze(1).expand(-1, 10, -1)\n        \n        # Triangular attention loop\n        context, _ = self.image_to_text_attn(disease_embed, combined_visual, combined_visual)\n        hypothesis, _ = self.text_to_disease_attn(context, disease_embed, disease_embed)\n        verified, _ = self.disease_to_image_attn(hypothesis, combined_visual, combined_visual)\n        \n        # Project to GPT-2 dimension\n        encoder_hidden_states = self.visual_to_gpt2(verified)\n        \n        # Text Generation\n        if input_ids is not None:\n            # Training mode - with cross-attention\n            outputs = self.text_decoder(\n                input_ids=input_ids,\n                attention_mask=attention_mask,\n                encoder_hidden_states=encoder_hidden_states,\n                labels=input_ids\n            )\n            return {\n                'loss': outputs.loss,\n                'logits': outputs.logits,\n                'disease_logits': disease_logits\n            }\n        else:\n            return {\n                'encoder_hidden_states': encoder_hidden_states,\n                'disease_logits': disease_logits\n            }\n    def classify_diseases(self, pixel_values):\n        vit_outputs = self.visual_encoder(pixel_values, output_hidden_states=True)\n        global_feature = vit_outputs.last_hidden_state[:, 0]\n\n        residual_out = self.residual_path(global_feature)\n        expansion_out = self.expansion_path(global_feature)\n        disease_features = residual_out + expansion_out\n\n        disease_logits = self.disease_classifier(disease_features)\n        return disease_logits\n\n    def generate(self, pixel_values, max_length=200, **kwargs):\n        vit_outputs = self.visual_encoder(pixel_values, output_hidden_states=True)\n\n        batch_size = pixel_values.size(0)\n\n        pixel_features = vit_outputs.last_hidden_state[:, 1:]\n        pixel_features = self.pixel_proj(pixel_features)\n\n        region_features = pixel_features.reshape(batch_size, 14, 14, 512)\n        region_features = nn.functional.avg_pool2d(\n            region_features.permute(0, 3, 1, 2),\n            kernel_size=4,\n            stride=4\n        ).permute(0, 2, 3, 1).reshape(batch_size, -1, 512)\n\n        organ_features = vit_outputs.last_hidden_state[:, 0:1]\n        organ_features = self.organ_proj(organ_features)\n\n        global_feature = vit_outputs.last_hidden_state[:, 0]\n        residual_out = self.residual_path(global_feature)\n        expansion_out = self.expansion_path(global_feature)\n        disease_features = residual_out + expansion_out\n\n        combined_visual = torch.cat([pixel_features, region_features, organ_features], dim=1)\n        disease_embed = disease_features.unsqueeze(1).expand(-1, 10, -1)\n\n        context, _ = self.image_to_text_attn(disease_embed, combined_visual, combined_visual)\n        hypothesis, _ = self.text_to_disease_attn(context, disease_embed, disease_embed)\n        verified, _ = self.disease_to_image_attn(hypothesis, combined_visual, combined_visual)\n\n        encoder_hidden_states = self.visual_to_gpt2(verified)\n\n        input_ids = torch.tensor([[50256]], device=pixel_values.device)\n\n        generated_ids = self.text_decoder.generate(\n            input_ids=input_ids,\n            max_length=max_length,\n            encoder_hidden_states=encoder_hidden_states,\n            pad_token_id=50256,\n            **kwargs,\n            repetition_penalty=1.2,\n            no_repeat_ngram_size=3\n        )\n\n        return generated_ids\n","metadata":{"trusted":true,"execution":{"iopub.status.busy":"2026-02-08T12:42:49.600431Z","iopub.execute_input":"2026-02-08T12:42:49.601428Z","iopub.status.idle":"2026-02-08T12:42:49.623624Z","shell.execute_reply.started":"2026-02-08T12:42:49.601392Z","shell.execute_reply":"2026-02-08T12:42:49.622835Z"}},"outputs":[],"execution_count":8},{"cell_type":"code","source":"print(\"Creating model with cross-attention enabled...\")\nmodel = LightweightRadiologyModel(num_diseases=14)\n\ntotal_params = sum(p.numel() for p in model.parameters())\ntrainable_params = sum(p.numel() for p in model.parameters() if p.requires_grad