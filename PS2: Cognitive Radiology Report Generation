impression'])\n        \n        if max_samples:\n            self.data = self.data.head(max_samples).reset_index(drop=True)\n        \n        # 80-20 split\n        split_idx = int(len(self.data) * 0.8)\n        if split == 'train':\n            self.data = self.data[:split_idx].reset_index(drop=True)\n        else:\n            self.data = self.data[split_idx:].reset_index(drop=True)\n        \n        os.environ['TRANSFORMERS_CACHE'] = '/kaggle/working/transformers_cache'\n        \n        self.image_processor = ViTImageProcessor.from_pretrained('google/vit-base-patch16-224')\n        self.tokenizer = GPT2Tokenizer.from_pretrained('gpt2')\n        self.tokenizer.pad_token = self.tokenizer.eos_token\n        \n        print(f\"{split} set: {len(self.data)} samples\")\n    \n    def _find_image_dir(self):\n        possible_paths = [\n            f'{self.data_root}/images/images_normalized',\n            f'{self.data_root}/images',\n            f'{self.data_root}',\n        ]\n        \n        for path in possible_paths:\n            if os.path.exists(path):\n                try:\n                    files = os.listdir(path)\n                    if any(f.endswith(('.png', '.dcm', '.jpg')) for f in files):\n                        return path\n                except:\n                    continue\n        \n        for root, dirs, files in os.walk(self.data_root):\n            if any(f.endswith('.png') for f in files):\n                return root\n        \n        raise ValueError(f\"No image directory found in {self.data_root}\")\n    \n    def __len__(self):\n        return len(self.data)\n    \n    def __getitem__(self, idx):\n        row = self.data.iloc[idx]\n        \n        img_filename = row['filename']\n        possible_paths = [\n            os.path.join(self.image_dir, img_filename),\n            os.path.join(self.image_dir, img_filename.replace('.dcm.png', '.png')),\n            os.path.join(self.image_dir, img_filename.split('/')[-1]),\n        ]\n        \n        # Load image\n        image = None\n        for img_path in possible_paths:\n            if os.path.exists(img_path):\n                try:\n                    image = Image.open(img_path).convert('RGB')\n                    break\n                except Exception as e:\n                    continue\n        \n        if image is None:\n            print(f\"Image not found: {img_filename}, using placeholder\")\n            image = Image.new('RGB', (224, 224), color='black')\n        \n        # Process image\n        pixel_values = self.image_processor(image, return_tensors='pt')['pixel_values'].squeeze(0)\n        \n        # Create report text\n        findings = str(row['findings']) if pd.notna(row['findings']) else \"No findings\"\n        impression = str(row['impression']) if pd.notna(row['impression']) else \"No impression\"\n        report = f\"FINDINGS: {findings}\\nIMPRESSION: {impression}\"\n        \n        # Tokenize\n        tokens = self.tokenizer(\n            report,\n            max_length=128,\n            padding='max_length',\n            truncation=True,\n            return_tensors='pt'\n        )\n        \n        disease_labels = torch.zeros(14)\n        \n        return {\n            'pixel_values': pixel_values,\n            'input_ids': tokens['input_ids'].squeeze(0),\n            'attention_mask': tokens['attention_mask'].squeeze(0),\n            'labels': tokens['input_ids'].squeeze(0),\n            'disease_labels': disease_labels,\n            'indication': row.get('indication', 'No indication')\n        }","metadata":{"trusted":true,"execution":{"iopub.status.busy":"2026-02-08T12:42:27.940172Z","iopub.execute_input":"2026-02-08T12:42:27.940601Z","iopub.status.idle":"2026-02-08T12:42:27.960521Z","shell.execute_reply.started":"2026-02-08T12:42:27.940573Z","shell.execute_reply":"2026-02-08T12:42:27.959454Z"}},"outputs":[],"execution_count":5},{"cell_type":"markdown","source":"## Dataset Creation","metadata":{}},{"ce